{
    "name": "pcsaini/laravel-api-response-builder",
    "description": "A super simple package allowing for consistent API responses throughout your Laravel application",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Prem Chand Saini",
            "email": "prem@signaturetech.in"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/http": "~7.0"
    },
    "require-dev": {
        "orchestra/testbench": "7.*",
        "phpunit/phpunit": "9.*"
    },
    "autoload": {
        "psr-4": {
            "Prem\\ResponseBuilder\\": "src/",
            "Prem\\ResponseBuilder\\Tests\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Prem\\ResponseBuilder\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Prem\\ResponseBuilder\\ResponseBuilderServiceProvider"
            ],
            "aliases": {
                "response_builder": "Prem\\ResponseBuilder\\Facades\\ResponseBuilder"
            }
        }
    }
}
